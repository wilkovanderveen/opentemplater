      <ot:CMYK cyan="0.4" magenta="0" yellow="0" black="0.10" />
      <ot:RGB red="200" green="0" blue="0" />
      <ot:PMS name="PMS302" />
    </ot:Color>
    <ot:Color key="green">
      <ot:CMYK cyan="0" magenta="0.4" yellow="0.2" black="0" />
      <ot:RGB red="0" green="200" blue="0" />
    </ot:Color>
  </ot:Colors>
  <ot:Fonts basePath="C:\Windows\Fonts">
    <ot:Font key="Arial">
      <ot:FontStyle style="Bold" file="arialbd.ttf" />
    </ot:Font>
  </ot:Fonts>
  <ot:PageTemplates>
    <ot:PageTemplate key="MyFirstStaticTemplate" width="210mm" height="297mm">
